#!/usr/bin/env python

import sys,json
from collections import OrderedDict


if len(sys.argv) < 2:
	fd_in=sys.stdin
else:
	fd_in=open(sys.argv[1], 'r')

data = json.load(fd_in)

if len(sys.argv) < 2:
	fd_out = sys.stdout
else:
	fd_in.close()
	fd_out=open(sys.argv[1],'w')

fd_out.write(json.dumps(data, indent=2, sort_keys=True))

if len(sys.argv) >= 1:
	fd_out.close()

